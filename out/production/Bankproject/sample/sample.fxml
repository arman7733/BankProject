      <Accordion GridPane.rowIndex="1" />
      <AnchorPane prefHeight="480.0" prefWidth="388.0" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
         <children>
            <Label layoutX="127.0" layoutY="14.0" prefHeight="56.0" prefWidth="134.0" text="USERNAME">
               <font>
                  <Font name="Cambria" size="25.0" />
               </font>
            </Label>
            <Label layoutX="127.0" layoutY="191.0" prefHeight="49.0" prefWidth="134.0" text="PASSWORD" textAlignment="CENTER">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <TextField fx:id="UserText" layoutX="118.0" layoutY="105.0" />
            <TextField fx:id="PassText" layoutX="118.0" layoutY="266.0" />
            <Button fx:id="resaultKey" layoutX="167.0" layoutY="335.0" mnemonicParsing="false" onAction="#userPassCheck" text="Check" textAlignment="CENTER" />
            <Label fx:id="ShowResault" layoutX="118.0" layoutY="402.0" prefHeight="32.0" prefWidth="152.0" textAlignment="CENTER" />
         </children>
      </AnchorPane>
