<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SigninAdmin">
    <children>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
        </GridPane>
        <AnchorPane prefHeight="503.0" prefWidth="362.0">
            <children>
                <Label fx:id="lblUsername" layoutX="125.0" layoutY="44.0" text="Username">
                    <font>
                        <Font size="25.0" />
                    </font>
                </Label>
                <Label fx:id="lblPassword" layoutX="127.0" layoutY="190.0" text="Password">
                    <font>
                        <Font size="25.0" />
                    </font>
                </Label>
                <Button fx:id="btnEnter" layoutX="142.0" layoutY="346.0" mnemonicParsing="false" onAction="#pressEnter" prefHeight="36.0" prefWidth="76.0" text="Enter" />
                <TextField fx:id="txtUsername" layoutX="107.0" layoutY="110.0" prefHeight="43.0" prefWidth="149.0" promptText="Username">
                    <font>
                        <Font size="16.0" />
                    </font>
                </TextField>
                <TextField fx:id="txtPassword" layoutX="107.0" layoutY="259.0" prefHeight="43.0" prefWidth="149.0" promptText="Password">
                    <font>
                        <Font size="18.0" />
                    </font>
                </TextField>
                <Label fx:id="lblshow" layoutX="46.0" layoutY="429.0" prefHeight="55.0" prefWidth="268.0" />
            </children>
        </AnchorPane>
    </children>
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
</GridPane>
